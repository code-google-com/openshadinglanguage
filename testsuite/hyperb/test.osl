#ifndef M_E
#define M_E 2.718281828459
#endif


shader
test ()
{
    {
        float a = -M_E;
        printf ("  cosh (%g) = %g\n", a, cosh(a));
        printf ("  sinh (%g) = %g\n", a, sinh(a));
        a = 0;
        printf ("  cosh (%g) = %g\n", a, cosh(a));
        printf ("  sinh (%g) = %g\n", a, sinh(a));
        a = M_E;
        printf ("  cosh (%g) = %g\n", a, cosh(a));
        printf ("  sinh (%g) = %g\n", a, sinh(a));
    }

    { 
        vector a = -M_E;
        printf ("  cosh (%g) = %g\n", a, cosh(a));
        printf ("  sinh (%g) = %g\n", a, sinh(a));
        a = 0;
        printf ("  cosh (%g) = %g\n", a, cosh(a));
        printf ("  sinh (%g) = %g\n", a, sinh(a));
        a = M_E;
        printf ("  cosh (%g) = %g\n", a, cosh(a));
        printf ("  sinh (%g) = %g\n", a, sinh(a));
    }

    printf (" varying:\n");
    {
        float a = u;
        printf ("  cosh (%g) = %g\n", a, cosh(a));
        printf ("  sinh (%g) = %g\n", a, sinh(a));
    }
    {
        color a = u;
        printf ("  cosh (%g) = %g\n", a, cosh(a));
        printf ("  sinh (%g) = %g\n", a, sinh(a));
    }
}
